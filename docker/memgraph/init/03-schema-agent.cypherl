CREATE CONSTRAINT ON (s:Session) ASSERT s.key IS UNIQUE;
CREATE CONSTRAINT ON (m:Message) ASSERT m.entryId IS UNIQUE;
CREATE CONSTRAINT ON (e:Entity) ASSERT e.key IS UNIQUE;
CREATE CONSTRAINT ON (d:AgentDoc) ASSERT d.key IS UNIQUE;

CREATE INDEX ON :Session(agentId);
CREATE INDEX ON :Session(updatedAt);
CREATE INDEX ON :AgentDoc(docType);

CREATE INDEX ON :Session(key);
CREATE INDEX ON :Session(sessionId);
CREATE INDEX ON :Message(sessionKey);
CREATE INDEX ON :Message(entryId);
CREATE INDEX ON :Entity(key);
